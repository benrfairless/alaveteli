<h2>Next steps</h2>

<ul class="wizard__next-steps">
  <% actions.each do |action| %>
  <li class="wizard__next-step" data-block="<%= action.id %>">
    <a class="wizard__next-step__title" href="#"><%= action.title %></a>
    <% if @info_request %>
      <%= render action, f: f %>
    <% else %>
      <%= render action %>
    <% end %>
  </li>
  <% end %>
</ul>
